{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "noEmit": true,
    "sourceMap": true
  },
  "include": [
    "./src/**/*.ts"
  ]
}
